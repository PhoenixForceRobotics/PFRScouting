<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fyrebirdscout11">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/scoutingapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="edu.phoenixforce.scouting.mobile.Settings"></activity>
        <activity android:name="edu.phoenixforce.scouting.mobile.AutoScore" />
        <activity android:name="edu.phoenixforce.scouting.mobile.TeleScore" />
        <activity android:name="edu.phoenixforce.scouting.mobile.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>